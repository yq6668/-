.image {
  position: relative;
  margin-bottom: -8px;
  z-index: -1;
}
.image .banner {
  width: 100%;
  opacity: 0.8;
}
.image .img-tit {
  position: absolute;
  top: 68px;
  left: 10px;
  font-size: 20px;
  font-weight: 600;
}
.image .img-tit .tit-1 {
  color: black;
}
.image .img-tit .tit-2 {
  font-size: 14px;
  color: black;
}
.overview {
  padding: 0 10px;
}
.overview .container {
  background-color: white;
  box-shadow: 0px 1rem 2rem 0px rgba(48, 55, 66, 0.15);
  transform: translate(0, -5 px);
}
.overview .container .lastupdatatime {
  font-size: 14px;
  margin-bottom: 10px;
}
.overview .container .lastupdatatime .time-text {
  font-size: 16px;
  color: #756d6d;
}
.yq-content .yqinfo {
  display: inline-block;
  width: 25%;
  text-align: center;
  font-size: 14px;
  border: solid 1px #f3f3f3;
  box-sizing: border-box;
}
.yq-content .yqinfo .content-tit {
  font-weight: 600;
  margin: 5px 0;
}
.yq-content .yqinfo .content-num {
  font-size: 20px;
  font-weight: 700;
  margin-bottom: 5px;
}
.yq-content .yqinfo .content-tips {
  display: inline-block;
}
.yq-content .yqinfo .add-num {
  display: inline-block;
  font-size: 16px;
  margin-bottom: 15px;
}
.map {
  width: 100%;
  height: 400px;
}
.map #mychart-dom-area {
  width: 100%;
}
.table .t-title {
  font-size: 22px;
  font-weight: 800;
}
.table .t-name {
  font-size: 14px;
  text-align: center;
  height: 40px;
  line-height: 40px;
}
.table .t-name .area {
  background-color: #ede7e7;
  display: inline-block;
  width: 20%;
  color: black;
}
.table .t-name .extant {
  background-color: #fcf2e8;
  display: inline-block;
  width: 20%;
  color: red;
}
.table .t-name .total {
  background-color: #fdeeee;
  display: inline-block;
  width: 20%;
  color: #d66262;
}
.table .t-name .dead {
  background-color: #f3f6f8;
  display: inline-block;
  width: 20%;
  color: grey;
}
.table .t-name .heal {
  background-color: #e9f7ec;
  display: inline-block;
  width: 20%;
  color: #3dae76;
}
.table .t-content {
  font-size: 16px;
  font-weight: 600;
  text-align: center;
  border-bottom: solid 1px grey;
  line-height: 50px;
}
.table .t-content .t-infoname {
  display: inline-block;
  font-size: 12px;
  width: 20%;
}
.table .t-content .t-info {
  display: inline-block;
  font-size: 16px;
  width: 20%;
}
.table .t-content .t-info-total {
  display: inline-block;
  width: 20%;
  position: relative;
}
.table .t-content .t-info-total .total-mes {
  line-height: 7px;
  height: fit-content;
}
.table .t-content .t-info-total .total-add {
  position: absolute;
  bottom: -36px;
  left: 10px;
  font-size: 12px;
  font-weight: 400;
  color: #dc0f00;
}
